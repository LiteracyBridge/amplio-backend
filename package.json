{
  "name": "amplio_suite_api",
  "version": "0.0.7",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "nest": "nest",
    "build": "nest build",
    "build:lambda": "nest build --config nest-cli.lambda.json --webpack; cp --verbose src/programs/spec/template.xlsx dist",
    "start:lambda": "node ./dist/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "typeorm-ts-node-commonjs  migration:run --dataSource ./src/data-source.js",
    "migration:revert": "typeorm-ts-node-commonjs  migration:revert --dataSource ./src/data-source.js",
    "migration:generate": "typeorm-ts-node-commonjs  migration:generate --dataSource ./src/data-source.js",
    "migration:create": "typeorm-ts-node-commonjs  migration:create",
    "console": "npm run build && node dist/console.js"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.654.0",
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/client-ses": "^3.666.0",
    "@codegenie/serverless-express": "^4.15.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/nestjs": "^8.32.0",
    "aws-jwt-verify": "^4.0.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "json-diff-ts": "^4.1.0",
    "jws": "^4.0.0",
    "luxon": "^3.5.0",
    "nestjs-console": "^9.0.0",
    "pg": "^8.13.0",
    "read-excel-file": "^5.8.5",
    "reflect-metadata": "^0.2.0",
    "remeda": "^2.14.0",
    "rxjs": "^7.8.1",
    "terser-webpack-plugin": "^5.3.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
