{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "index",
  "compilerOptions": {
    "webpack": true,
    "webpackConfigPath": "webpack.config.js",
    "watchAssets": true,
    "deleteOutDir": true,
    "assets": [
      {
        "include": "programs/spec/template.xlsx",
        "outDir": "dist/template.xlsx"
      }
    ]
  }
}
